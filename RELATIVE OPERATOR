> < >= <= == !=

5>4  //1
5>5  //0
5<4   //0
5<5   //0
5>=4   //1
5>=5   //1
5<=4   //0
5<=5    //1
5==5    //1
5!=5    //0
5=5   //1 value required
5>;   //error

calculate simple interest?
10%
x+(x* 10/100)

#include <stdio.h>
int main()
{
  int amount=100;
  int per=10;
  int time=8;
  int interest=amount+(amount*per/100);
  printf("%d",interest*time);

return 0;
}

