scanf("%d", &month); 
printf("Enter year: ");
scanf("%d", &year);
switch (month) {
    case 1: case 3: case 5: case 7: case 8: case 10: case 12:
printf("31 days\n");
break;
case 4: case 6: case 9: case 11:
printf("30 days\n");
break;
case 2:
